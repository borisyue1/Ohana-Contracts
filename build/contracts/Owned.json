  "source": "pragma solidity ^0.4.24;\n\nimport \"./SafeMath.sol\";\n\n\ncontract Owned {\n\n    using SafeMath for uint256; // attaches the library's functions to uint256 type\n\n    address public owner;\n\n    constructor() public {\n        owner = msg.sender;\n    }\n\n    modifier onlyOwner {\n        require(msg.sender == owner, \"Only the root account can execute this action\");\n        _;\n    }\n}",
